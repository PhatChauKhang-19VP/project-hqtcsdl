<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17"
            fx:controller="pck.dbms.fe.partner.home.Controller">
    <top>
        <Pane prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #A8ACFF;" BorderPane.alignment="CENTER"/>
    </top>
    <bottom>
        <Pane prefHeight="50.0" prefWidth="200.0" style="-fx-background-color: #A8ACFF;" BorderPane.alignment="CENTER"/>
    </bottom>
    <center>
        <Pane prefHeight="600.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <children>
                <Label layoutX="327.0" layoutY="100.0" text="Đối tác">
                    <font>
                        <Font name="Arial" size="48.0"/>
                    </font>
                </Label>
                <AnchorPane layoutX="114.0" layoutY="200.0" prefHeight="120.0" prefWidth="575.0">
                    <children>
                        <Button fx:id="btnContract" mnemonicParsing="false" prefHeight="50.0" prefWidth="275.0"
                                style="-fx-font-size: 24;" text="Hợp đồng"
                                onAction="#onBtnContractClick"
                        >
                            <font>
                                <Font name="Arial" size="24.0"/>
                            </font>
                        </Button>
                        <Button fx:id="btnManageProduct" layoutX="300.0" mnemonicParsing="false" prefHeight="50.0"
                                prefWidth="275.0" style="-fx-font-size: 24;" text="Quản lí sản phẩm"
                                onAction="#onBtnManageProductClick">
                            <font>
                                <Font size="24.0"/>
                            </font>
                        </Button>
                        <Button fx:id="btnManageOrder" layoutY="70.0" mnemonicParsing="false" prefHeight="50.0"
                                prefWidth="275.0" style="-fx-font-size: 24;" text="Quản lí đơn hàng"
                                onAction="#onBtnManageOrderClick">
                            <font>
                                <Font name="Arial" size="24.0"/>
                            </font>
                        </Button>
                    </children>
                </AnchorPane>
            </children>
        </Pane>
    </center>
</BorderPane>
